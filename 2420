        plt.xlabel('width')
        plt.ylabel('height')
        
        x = np.array(x)
        a, b, c = np.polyfit(x, y, 2) #최소자승법으로 2차곡선을 알려준다.
        ax.plot(x, a*x**2+b*x+c, color = 'teal')
              
pg.draw()` -> pg.draw()
